{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [],
    "tasks": {
        "dev": {
            "cache": false,
            "persistent": true
        },
        "build": {
            "outputs": [
                "dist/**"
            ],
            "dependsOn": [
                "^build"
            ]
        },
        "test": {
            "dependsOn": [
                "build"
            ]
        },
        "lint": {}
    }
}